public class Test
{
public static void main(String [] args)
{
int num=101;
String str="20060320";
String[] studentNum=new String[20]; // 存放学号
for(int i=0; i<20; i++)
{
studentNum[i]=str+num; // 生成各学号，其中的加号起到连接作用。
num++;
}
for(int i=0; i<20; i++)
{
System.out.print(studentNum[i]+" ");
if((i+1)%5==0) System.out.println(" ");// 输出5 个后换行
}

}
}
